<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fwp.moiveguide">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:name=".myapp.MyApplication"
        android:theme="@style/AppMainTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".domain.DetailsActivity"
            android:theme="@style/Anim_translate" />

        <activity android:name=".domain.MineActivity"
            android:theme="@style/Anim_translate" />
        <activity android:name=".domain.LoginActivity"
            android:theme="@style/Anim_translate" />
        <activity android:name=".domain.UserDataActivity"
            android:theme="@style/Anim_translate" />
    </application>

</manifest>